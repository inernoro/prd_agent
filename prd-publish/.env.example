# Authentication
PUBLISH_USERNAME=admin
PUBLISH_PASSWORD=your-secret-password
PUBLISH_JWT_SECRET=your-jwt-secret-key-change-this

# Server
PUBLISH_PORT=3939
PUBLISH_HOST=0.0.0.0

# Git Configuration
PUBLISH_REPO_PATH=/path/to/your/repo
PUBLISH_BRANCH=main

# Execution
PUBLISH_EXEC_SCRIPT=./exec.sh
PUBLISH_EXEC_TIMEOUT=300000

# Retry Configuration
PUBLISH_AUTO_RETRY=false
PUBLISH_RETRY_COUNT=3
PUBLISH_RETRY_DELAY=5000
