{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Jwt": {
    "Secret": "dev-only-change-me-32bytes-minimum!!",
    "Issuer": "prdagent",
    "Audience": "prdagent",
    "ExpirationHours": 24,
    "AccessTokenMinutes": 60
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Authentication": "Error",
        "System": "Warning",
        "PrdAgent.Infrastructure.LLM.HttpLoggingHandler": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Assets": {
    "Provider": "tencentCos",
    "DisableLocal": true
  }
}
