{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDB": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "prdagent"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Jwt": {
    "Issuer": "prdagent",
    "Audience": "prdagent",
    "ExpirationHours": 24,
    "AccessTokenMinutes": 60
  },
  "Session": {
    "TimeoutMinutes": 30
  },
  "LLM": {
    "Provider": "Claude",
    "MaxRetries": 3,
    "TimeoutSeconds": 60,
    "ImageGenTimeoutSeconds": 600,
    "ImageGenDownloadTimeoutSeconds": 120
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:1420",
      "http://localhost:8000",
      "http://localhost:5173",
      "http://localhost:4173",
      "http://127.0.0.1:1420",
      "http://127.0.0.1:8000",
      "http://127.0.0.1:5173",
      "http://127.0.0.1:4173",
      "http://[::1]:1420",
      "http://[::1]:8000",
      "http://[::1]:5173",
      "http://[::1]:4173"
    ]
  },
  "Assets": {
    "Provider": "tencentCos",
    "DisableLocal": true
  },
  "OpenPlatform": {
    "TestApiKey": "sk-test-permanent-key-for-testing-only"
  },
  "RateLimit": {
    "MaxRequestsPerMinute": 600,
    "MaxConcurrentRequests": 100
  }
}



