{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:allow-listen",
    "core:event:default",
    "shell:allow-open",
    "clipboard-manager:allow-write-text",
    "clipboard-manager:allow-write-image",
    "updater:default"
  ]
}




